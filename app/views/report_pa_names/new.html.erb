<% title(t('.page_title')) %>

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <h1 class="govuk-heading-l"><%= t(".heading") %></h1>
    <%= form_for @report_pa_name, :as => :report_pa_name do |f| %>
        <%= f.govuk_error_summary %>
        <%= f.govuk_text_field :pa_name %>
        <%= f.govuk_submit %>
        <%= govuk_button_link_to t(".cancel_btn"), report_pa_names_path, secondary: true %>  
    <% end %>
  </div>
</div>
